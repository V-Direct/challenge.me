#!/usr/bin/env node

/**
 * Module dependencies.
 */

const app = require("../server");

/**
 * Get port from environment and store in Express.
 */

const port = process.env.PORT || "3000";
app.set("port", port);

/**
 * Create HTTP server.
 */
app.listen(port, () => console.log("Server running on localhost:" + port));
